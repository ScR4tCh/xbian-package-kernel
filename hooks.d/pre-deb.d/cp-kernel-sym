

kver="$(make -C ../build/$config_platform_branch/working kernelversion|grep -v make:)+"

case $config_platform_branch in
    imx6)
        cp ../build/$config_platform_branch/working/System.map ./boot/System.map
        cp ../build/$config_platform_branch/working/arch/arm/boot/zImage ./boot/zImage
        ;;
    rpi*)
        cp ../build/$config_platform_branch/working/System.map ./boot/System.map-$kver
        cp ../build/$config_platform_branch/working/arch/arm/boot/zImage ./boot/kernel.img-$kver
        ;;
    *)
        ;;
esac

